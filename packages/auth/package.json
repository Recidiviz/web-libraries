    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-url": "^10.1.1",
    "react-scripts": "5.0.0",
    "rollup": "^2.75.7",
